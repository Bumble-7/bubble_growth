FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
  top
  {
      type            calculated;
      value           $internalField;
  }

  bottom
  {
      type calculated;
      value           $internalField;
  }

  right
  {
      type            calculated;
      value           $internalField;
  }
  front
  {
    type wedge;
  }
  back
  {
    type wedge;
  }
  axis
  {
    type empty;
  }
}

// ************************************************************************* //
